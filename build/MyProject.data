#version 300 es
precision highp float;
out vec4 FragColor;

uniform vec3 myColor;
void main() {
    FragColor = vec4(myColor, 1.0);
}#version 300 es
layout(location = 0) in highp vec3 aPos;

uniform highp mat4 cMatrix;
uniform highp mat4 wMatrix;

void main()
{
    gl_Position = cMatrix * wMatrix * vec4(aPos, 1.0);
}